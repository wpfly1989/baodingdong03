            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:textSize="20sp"
            android:layout_toRightOf="@id/gaobeidiandongjiaoliupeidianId"
            android:layout_alignBaseline="@id/gaobeidiandongjiaoliupeidianId"
