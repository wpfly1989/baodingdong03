            android:text="UPS\n蓄电池"
            android:textColor="#000000"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />

        <Button
            android:id="@+id/gaobeidiandongjiaoliupeidianId"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
